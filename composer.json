{
	"name" : "forxer/wampi",
	"type" : "project",
	"license" : "MIT",
	"description" : "Convenient replacement for the default index.php of Wampserver",
	"keywords" : [
		"php",
		"wampserver",
		"index",
		"localhost"
	],
	"authors" : [{
			"name" : "Vincent Garnier",
			"email" : "forxer@gmail.org",
			"homepage" : "http://forx.fr"
		}
	],
	"require" : {
		"php" : ">=5.5",
		"doctrine/dbal" : "~2.4",
		"filp/whoops" : "~1.1",
		"forxer/tao" : "~0.8",
		"mrclay/minify" : "~2.2",
		"raveren/kint" : "~0.9",
		"symfony/templating" : "~2.5",
		"symfony/translation" : "~2.5"
	},
	"autoload" : {
		"psr-4" : {
			"Application\\" : "Application"
		}
	},
	"scripts" : {
		"post-install-cmd" : [
			"bower install"
		],
		"post-update-cmd" : [
			"bower install"
		]
	}
}